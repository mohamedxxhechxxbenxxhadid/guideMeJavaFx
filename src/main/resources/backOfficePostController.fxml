<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: #dadada;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controllers.backOfficePostController">
   <children>
      <Button fx:id="deleteButton" layoutX="214.0" layoutY="762.0" mnemonicParsing="false" onAction="#deletePost" text="Delete selected post" textFill="RED" />
      <TableView fx:id="tableViewid" prefHeight="738.0" prefWidth="1200.0">
         <columns>
            <TableColumn fx:id="titleId" text="Title" />
            <TableColumn fx:id="descriptionId" prefWidth="540.0" text="Description" />
            <TableColumn fx:id="approvedId" prefWidth="108.0" text="Approved" />
            <TableColumn fx:id="bigPostId" prefWidth="102.0" text="Big Post" />
            <TableColumn fx:id="downId" prefWidth="90.0" text="Down" />
            <TableColumn fx:id="upId" prefWidth="95.0" text="Up" />
            <TableColumn fx:id="createdId" prefWidth="184.0" text="Created" />
         </columns>
      </TableView>
      <Button fx:id="approveButtonId" layoutX="779.0" layoutY="762.0" mnemonicParsing="false" onAction="#approvePost" prefHeight="24.0" prefWidth="192.0" text="approve" textFill="#14521a" />
   </children>
</AnchorPane>
